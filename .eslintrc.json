{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true, "printWidth": 80, "jsx-filename-extension": false}],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-props-no-spreading": [1, {"custom": "ignore"}],
        "no-use-before-define": [2, {"variables": false}]
    }
}